# App
Z3_APP_NAME=Z3
Z3_PUBLIC_URL=http://localhost:3000
Z3_SESSION_SECRET=change-me-long-random # used for session encryption

# Default MinIO profile (dev-only convenience; optional)
Z3_MINIO_ENDPOINT=http://localhost:9000
Z3_MINIO_REGION=us-east-1
Z3_MINIO_USE_SSL=false
Z3_MINIO_VERIFY_TLS=false

# Dev AK/SK (ONLY for local dev; do not use in prod; read by dev server to prefill forms)
Z3_MINIO_ACCESS_KEY=
Z3_MINIO_SECRET_KEY=

# OIDC (if MinIO + IdP are configured; used by NextAuth or equivalent)
Z3_OIDC_ISSUER_URL=
Z3_OIDC_CLIENT_ID=
Z3_OIDC_CLIENT_SECRET=
Z3_OIDC_CALLBACK_URL=http://localhost:3000/api/auth/callback

# Telemetry / logging
Z3_LOG_LEVEL=info
Z3_TELEMETRY_DISABLED=true
